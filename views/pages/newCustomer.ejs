<!DOCTYPE html>
<html lang="en">

<script>
$('.datepicker').pickadate({
	selectMonths: true, // Creates a dropdown to control month
	selectYears: 15 // Creates a dropdown of 15 years to control year
});
</script>

<head>
	<title>New Customer</title>
	<%- include header.ejs %>
</head>
<body>
	<nav>
		<div class="nav-wrapper teal darken-1">
			<a href="/salesRep" class="brand-logo storeLogo">"\<"New Customer</a>
			<ul id="nav-mobile" class="right hide-on-med-and-down">
				<li><a href="/">Sign Out</a></li>
			</ul>
		</div>
	</nav>
	<div class="w3-container">
		<div class="row">
			<form class="col s12">
				<div class="row">
					<div class="input-field col">
						<i class="material-icons prefix">business</i>
	          <input id="lastName" type="text" class="validate">
	          <label for="lastName">Company Name</label>
					</div>
					<div class="input-field col">
						<i class="material-icons prefix">account_circle</i>
	          <input id="firstName" type="text" class="validate">
	          <label for="firstName">Contact Name</label>
					</div>
					<div class="input-field col">
						<i class="material-icons prefix">email</i>
	          <input id="email" type="text" class="validate">
	          <label for="email">Email Address</label>
					</div>
					<div class="input-field col">
						<i class="material-icons prefix">phone</i>
	          <input id="phone" type="text" class="validate">
	          <label for="phone">Phone Number</label>
					</div>
				</div>

				<div class="row">
					<div class="input-field col">
						<i class="material-icons prefix">payment</i>
	          <input id="card_number" type="text" class="validate">
	          <label for="card_number">Card Number</label>
					</div>
					<div class="input-field col">
						<i class="material-icons prefix">payment</i>
	          <input id="cvc" type="text" class="validate">
	          <label for="cvc">CVC Code</label>
					</div>
					<div class="input-field col">
						<i class="material-icons prefix">today</i>
	          <input id="date" type="date" class="datepicker">
	          <label for="date">Expiration Date</label>
					</div>
					<div class="input-field col">
						<i class="material-icons prefix">location_on</i>
	          <input id="billingAddress" type="text" class="validate">
	          <label for="billingAddress">Billing Address</label>
					</div>
				</div>
			</form>
		</div>
	</div>
</body>
</html>
